minimizeCorner.CornerRadius = UDim.new(0, 8)
minimizeCorner.Parent = minimizeButton

-- FunÃ§Ã£o para minimizar/restaurar
local function toggleMinimize()
    panelMinimized = not panelMinimized
    if panelMinimized then
        contentFrame.Visible = false
        titleSeparator.Visible = false
        minimizeButton.Text = "+"
        minimizeButton.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
    else
        contentFrame.Visible = true
        titleSeparator.Visible = true
        minimizeButton.Text = "_"
        minimizeButton.BackgroundColor3 = Color3.fromRGB(150, 0, 150)
    end
end

minimizeButton.MouseButton1Click:Connect(toggleMinimize)
minimizeButton.TouchTap:Connect(toggleMinimize)

-- Separator line after title
