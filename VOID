-- Bot√£o para alternar entre abas (apenas Mobile)
if isMobile then
    local tabSwitchContainer = Instance.new("Frame")
    tabSwitchContainer.Name = "TabSwitch_Container"
    tabSwitchContainer.Size = UDim2.new(1, -10, 0, 35)
    tabSwitchContainer.Position = UDim2.new(0, 5, 1, -45)
    tabSwitchContainer.BackgroundTransparency = 1
    tabSwitchContainer.Parent = contentFrame

    local leftTabButton = Instance.new("TextButton")
    leftTabButton.Name = "LeftTab_Button"
    leftTabButton.Size = UDim2.new(0.5, -2, 1, 0)
    leftTabButton.Position = UDim2.new(0, 0, 0, 0)
    leftTabButton.BackgroundColor3 = Color3.fromRGB(150, 0, 150)
    leftTabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    leftTabButton.TextSize = 12
    leftTabButton.Font = Enum.Font.GothamBold
    leftTabButton.Text = "‚ö° ESQUERDO"
    leftTabButton.BorderSizePixel = 0
    leftTabButton.AutoButtonColor = false
    leftTabButton.Parent = tabSwitchContainer

    local leftTabCorner = Instance.new("UICorner")
    leftTabCorner.CornerRadius = UDim.new(0, 5)
    leftTabCorner.Parent = leftTabButton

    local rightTabButton = Instance.new("TextButton")
    rightTabButton.Name = "RightTab_Button"
    rightTabButton.Size = UDim2.new(0.5, -2, 1, 0)
    rightTabButton.Position = UDim2.new(0.5, 2, 0, 0)
    rightTabButton.BackgroundColor3 = Color3.fromRGB(50, 20, 60)
    rightTabButton.TextColor3 = Color3.fromRGB(200, 200, 200)
    rightTabButton.TextSize = 12
    rightTabButton.Font = Enum.Font.GothamBold
    rightTabButton.Text = "üî• FUN√á√ïES"
    rightTabButton.BorderSizePixel = 0
    rightTabButton.AutoButtonColor = false
    rightTabButton.Parent = tabSwitchContainer

    local rightTabCorner = Instance.new("UICorner")
    rightTabCorner.CornerRadius = UDim.new(0, 5)
    rightTabCorner.Parent = rightTabButton

    local function switchToLeft()
        leftPanel.Visible = true
        rightPanel.Visible = false
        leftTabButton.BackgroundColor3 = Color3.fromRGB(150, 0, 150)
        leftTabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        rightTabButton.BackgroundColor3 = Color3.fromRGB(50, 20, 60)
        rightTabButton.TextColor3 = Color3.fromRGB(200, 200, 200)
    end

    local function switchToRight()
        leftPanel.Visible = false
        rightPanel.Visible = true
        leftTabButton.BackgroundColor3 = Color3.fromRGB(50, 20, 60)
        leftTabButton.TextColor3 = Color3.fromRGB(200, 200, 200)
        rightTabButton.BackgroundColor3 = Color3.fromRGB(150, 0, 150)
        rightTabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    end

    leftTabButton.MouseButton1Click:Connect(switchToLeft)
    leftTabButton.TouchTap:Connect(switchToLeft)
    
    rightTabButton.MouseButton1Click:Connect(switchToRight)
    rightTabButton.TouchTap:Connect(switchToRight)

    switchToLeft()
end

-- ========== IMPLEMENTA√á√ÉO DAS FUN√á√ïES ==========

-- Speed Boost
RunService.RenderStepped:Connect(function()
    if speedBoostEnabled and character:FindFirstChild("Humanoid") then
        local humanoid = character.Humanoid
        humanoid.WalkSpeed = 16 + (speedBoostValue / 100) * 50
    elseif character:FindFirstChild("Humanoid") then
        character.Humanoid.WalkSpeed = 16
    end
end)

-- Anti Ragdoll
local function antiRagdoll()
    if antiRagdollEnabled and character:FindFirstChild("Humanoid") then
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BodyVelocity") or part:IsA("BodyGyro") or part:IsA("BodyPosition") then
                part:Destroy()
            end
        end
    end
end

RunService.Heartbeat:Connect(antiRagdoll)

-- Spin Bot
RunService.RenderStepped:Connect(function()
    if spinBotEnabled and character:FindFirstChild("HumanoidRootPart") then
        character.HumanoidRootPart.CFrame = character.HumanoidRootPart.CFrame * CFrame.Angles(0, math.rad(spinSpeedValue / 5), 0)
    end
end)

-- Gravity Control
RunService.Heartbeat:Connect(function()
    if galaxyModeEnabled then
        workspace.Gravity = (gravityValue / 100) * 196.2
    else
        workspace.Gravity = 196.2
    end
end)

-- Hop Power (Jump Height)
RunService.RenderStepped:Connect(function()
    if character:FindFirstChild("Humanoid") then
        character.Humanoid.JumpHeight = (hopPowerValue / 100) * 50
    end
end)

-- Galaxy Sky Bright
local lightingConnection
local function updateGalaxySky()
    local lighting = game:GetService("Lighting")
    if galaxySkyBrightEnabled then
        lighting.Ambient = Color3.fromRGB(255, 100, 200)
        lighting.OutdoorAmbient = Color3.fromRGB(255, 100, 200)
        lighting.ClockTime = 12
    else
        lighting.Ambient = Color3.fromRGB(200, 200, 200)
        lighting.OutdoorAmbient = Color3.fromRGB(200, 200, 200)
    end
end

RunService.Heartbeat:Connect(updateGalaxySky)

-- Unwalk
RunService.RenderStepped:Connect(function()
    if unwalkEnabled and character:FindFirstChild("Humanoid") then
        character.Humanoid.WalkSpeed = 0
    end
end)

-- Auto Steal (Sistema melhorado)
local stealDebounce = false
local function autoStealFunction()
    if not autoStealEnabled or stealDebounce then return end
    
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if humanoidRootPart then
        for _, obj in pairs(workspace:FindPartBySensor(humanoidRootPart.Position, 20)) do
            if obj and obj.Parent and obj.Parent:FindFirstChild("Humanoid") then
                stealDebounce = true
                wait(0.5)
                stealDebounce = false
            end
        end
    end
end

RunService.Heartbeat:Connect(autoStealFunction)

-- Spam Bat
local batSpamDebounce = false
local function spamBatFunction()
    if not spamBatEnabled or batSpamDebounce then return end
    
    local tool = character:FindFirstChildOfClass("Tool")
    if tool then
        batSpamDebounce = true
        tool:Activate()
        wait(0.1)
        batSpamDebounce = false
    end
end

RunService.Heartbeat:Connect(spamBatFunction)

-- Bat Aimbot
RunService.RenderStepped:Connect(function()
    if batAimbotEnabled and character:FindFirstChild("HumanoidRootPart") then
        local humanoidRootPart = character.HumanoidRootPart
        local closestPlayer = nil
        local closestDistance = 100
        
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer ~= player and otherPlayer.Character then
                local distance = (otherPlayer.Character.HumanoidRootPart.Position - humanoidRootPart.Position).Magnitude
                if distance < closestDistance then
                    closestDistance = distance
                    closestPlayer = otherPlayer
                end
            end
        end
        
        if closestPlayer and closestPlayer.Character then
            humanoidRootPart.CFrame = CFrame.new(humanoidRootPart.Position, closestPlayer.Character.HumanoidRootPart.Position)
        end
    end
end)

-- Restam Auto
local function restamAutoFunction()
    if not restamAutoEnabled then return end
    
    if character:FindFirstChild("Humanoid") and character.Humanoid.Health < 50 then
        -- Procurar por estamina ou regenera√ß√£o
        for _, item in pairs(workspace:FindPartBySensor(character.HumanoidRootPart.Position, 30)) do
            if item and item.Name:lower():find("stam") then
                -- Coletar item
            end
        end
    end
end

RunService.Heartbeat:Connect(restamAutoFunction)

-- Optimizer + XRay
local function optimizerXRay()
    if not optimizerXRayEnabled then return end
    
    -- Desabilitar texturas para melhorar performance
    for _, part in pairs(workspace:FindPartBySensor(character.HumanoidRootPart.Position, 50)) do
        if part and part:IsA("BasePart") then
            part.TextureID = ""
            part.Transparency = 0.5
        end
    end
end

RunService.Heartbeat:Connect(optimizerXRay)

-- Auto Right (Manuten√ß√£o autom√°tica)
local function autoRightFunction()
    if not autoRightEnabled then return end
    
    if character:FindFirstChild("HumanoidRootPart") then
        character.HumanoidRootPart.CFrame = character.HumanoidRootPart.CFrame * CFrame.Angles(0, 0, 0)
    end
end

RunService.Heartbeat:Connect(autoRightFunction)

-- Speed While Stealing
RunService.RenderStepped:Connect(function()
    if autoStealEnabled and character:FindFirstChild("Humanoid") then
        character.Humanoid.WalkSpeed = 16 + (stealSpeedValue / 100) * 30
    end
end)

-- Lidar com respawn do character
player.CharacterAdded:Connect(function(newCharacter)
    character = newCharacter
end)

-- Fechar painel com atalho (PC)
if isPC then
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed then return end
        if input.KeyCode == Enum.KeyCode.P then
            mainPanel.Visible = not mainPanel.Visible
        end
    end)
end

print("‚ú® VOID Panel carregado com sucesso! ‚ú®")
