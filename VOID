-- Bot√£o SAVE CONFIG
local saveContainer = Instance.new("Frame")
saveContainer.Name = "SaveConfig_Container"
saveContainer.Size = UDim2.new(1, -10, 0, 35)
saveContainer.Position = UDim2.new(0, 5, 0, yPos)
saveContainer.BackgroundTransparency = 1
saveContainer.Parent = leftScroll

local saveButton = Instance.new("TextButton")
saveButton.Name = "SaveConfig_Button"
saveButton.Size = UDim2.new(1, 0, 1, 0)
saveButton.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
saveButton.TextColor3 = Color3.fromRGB(255, 255, 255)
saveButton.TextSize = isMobile and 11 or 12
saveButton.Font = Enum.Font.GothamBold
saveButton.Text = "üíæ SAVE CONFIG"
saveButton.BorderSizePixel = 0
saveButton.AutoButtonColor = false
saveButton.Parent = saveContainer

local saveCorner = Instance.new("UICorner")
saveCorner.CornerRadius = UDim.new(0, 5)
saveCorner.Parent = saveButton

local function saveConfig()
    local config = {
        speedBoostEnabled = speedBoostEnabled,
        speedBoostValue = speedBoostValue,
        antiRagdollEnabled = antiRagdollEnabled,
        spinBotEnabled = spinBotEnabled,
        spinSpeedValue = spinSpeedValue,
        spamBatEnabled = spamBatEnabled,
        autoStealEnabled = autoStealEnabled,
        batAimbotEnabled = batAimbotEnabled,
        galaxySkyBrightEnabled = galaxySkyBrightEnabled,
        galaxyModeEnabled = galaxyModeEnabled,
        gravityValue = gravityValue,
        hopPowerValue = hopPowerValue,
        speedWhileStealingValue = speedWhileStealingValue,
        stealSpeedValue = stealSpeedValue,
        unwalkEnabled = unwalkEnabled,
        optimizerXRayEnabled = optimizerXRayEnabled,
        restamAutoEnabled = restamAutoEnabled,
        autoRightEnabled = autoRightEnabled
    }
    
    -- Feedback visual
    saveButton.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
    saveButton.Text = "‚úÖ SALVO!"
    wait(1)
    saveButton.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
    saveButton.Text = "üíæ SAVE CONFIG"
    
    print("‚ú® Config salvo com sucesso! ‚ú®")
end

saveButton.MouseButton1Click:Connect(saveConfig)
saveButton.TouchTap:Connect(saveConfig)

leftScroll.CanvasSize = UDim2.new(0, 0, 0, yPos + 50)

