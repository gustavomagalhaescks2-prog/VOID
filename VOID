-- VOID Panel - Roblox Lua Script (PC + MOBILE)
-- Script para o jogo "Steal a Brainrot"

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local TouchInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local mouse = player:GetMouse()
local character = player.Character or player.CharacterAdded:Wait()

-- Deteccionar plataforma
local isMobile = UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled
local isPC = not isMobile

-- Vari√°veis de controle
local speedBoostEnabled = false
local speedBoostValue = 1
local antiRagdollEnabled = false
local spinBotEnabled = false
local spinSpeedValue = 50
local spamBatEnabled = false
local autoStealEnabled = false
local batAimbotEnabled = false
local galaxySkyBrightEnabled = false
local galaxyModeEnabled = false
local gravityValue = 100
local hopPowerValue = 50
local speedWhileStealingValue = 1
local stealSpeedValue = 50
local unwalkEnabled = false
local optimizerXRayEnabled = false
local restamAutoEnabled = false
local autoRightEnabled = false

-- Criar ScreenGui com suporte a diferentes tamanhos de tela
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "VOID_Panel"
screenGui.ResetOnSpawn = false
screenGui.SafeAreaCompatibility = Enum.SafeAreaCompatibility.FullscreenExtension
screenGui.Parent = player:WaitForChild("PlayerGui")

-- Painel principal (responsivo para mobile e pc)
local mainPanel = Instance.new("Frame")
mainPanel.Name = "MainPanel"

if isMobile then
    mainPanel.Size = UDim2.new(0.95, 0, 0.9, 0)
    mainPanel.Position = UDim2.new(0.025, 0, 0.05, 0)
else
    mainPanel.Size = UDim2.new(0, 500, 0, 600)
    mainPanel.Position = UDim2.new(0.5, -250, 0.5, -300)
end

mainPanel.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
mainPanel.BorderSizePixel = 0
mainPanel.Parent = screenGui

-- Adicionar borda roxo/vermelho
local border = Instance.new("UIStroke")
border.Color = Color3.fromRGB(150, 0, 150)
border.Thickness = 2
border.Parent = mainPanel

-- Corner radius
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 10)
corner.Parent = mainPanel

-- T√≠tulo do painel
local titleLabel = Instance.new("TextLabel")
