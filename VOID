else
    rightPanel.Size = UDim2.new(0.5, -5, 1, 0)
    rightPanel.Position = UDim2.new(0.5, 5, 0, 0)
end

rightPanel.BackgroundTransparency = 1
rightPanel.Parent = contentFrame

